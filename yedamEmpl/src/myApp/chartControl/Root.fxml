<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="250" prefWidth="850"
	xmlns:fx="http://javafx.com/fxml/1"
	xmlns="http://javafx.com/javafx/15.0.1"
	fx:controller="myApp.chartControl.RootController">

	<children>
		<PieChart fx:id="pieChart" layoutX="5.0">
		</PieChart>
		<BarChart fx:id="barChart" layoutX="530.0">
			<xAxis>
				<CategoryAxis side="BOTTOM" />
			</xAxis>
			<yAxis>
				<NumberAxis side="LEFT" />
			</yAxis>
		</BarChart>
		<!-- <AreaChart fx:id="areaChart"> <xAxis> <CategoryAxis side="BOTTOM" 
			/> </xAxis> <yAxis> <CategoryAxis side="LEFT" /> </yAxis> </AreaChart> -->
	</children>
</AnchorPane>
